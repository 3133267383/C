#include <stdio.h>
char stack[512];
int top = 0;

void push(char c);
char pop(void);
int is_empty(void);

int main (void)
{
	
	return 0;
}

void push(char c)  //ru zhan
{
	stack[top++] = c;
}

char pop(void)	//chu zhan
{
	return stack[--top];
}

int is_empty(void) 	//jian ce zhan wei kong
{
	return top == 0;
}
